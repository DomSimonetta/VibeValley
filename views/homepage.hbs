<!-- Loop over each object in the playlists array -->
{{#each playlists as |playlist|}}
<div class="row mb-4 project">
    <div class="col-md-5">
      <h3>{{playlist.title}}</h3>
      <p>{{playlist.mood}}</p>
      <p>{{playlist.description}}</p>
      <p>Created by {{playlist.user.username}}</p>
    </div>
  </div>
{{/each}}